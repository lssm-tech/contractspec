{
  "name": "@contractspec/lib.ui-link",
  "version": "1.46.1",
  "description": "Deep linking utilities for navigation",
  "keywords": [
    "contractspec",
    "linking",
    "navigation",
    "deep-link",
    "ui",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/link.js",
  "module": "./dist/link.js",
  "types": "./dist/link.d.ts",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run build:bundle && bun run build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun run build:bundle --watch",
    "clean": "rm -rf dist",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint ui --fix",
    "lint:check": "eslint ui"
  },
  "dependencies": {
    "@contractspec/lib.ui-kit-core": "workspace:*",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "@contractspec/tool.tsdown": "workspace:*",
    "tsdown": "^0.18.3",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./dist/link.js",
    "./*": "./*"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/link.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/ui-link"
  },
  "homepage": "https://contractspec.io"
}

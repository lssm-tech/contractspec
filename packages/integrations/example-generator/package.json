{
  "name": "@contractspec/integration.example-generator",
  "version": "27.0.0",
  "description": "Example plugin: Markdown documentation generator for ContractSpec specs",
  "keywords": [
    "contractspec",
    "plugin",
    "generator",
    "markdown",
    "documentation",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./config": "./src/config.ts",
    "./generator": "./src/generator.ts",
    "./types": "./src/types.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "dev": "contractspec-bun-build dev",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test --pass-with-no-tests",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contractspec/lib.contracts-spec": "workspace:*",
    "@contractspec/lib.schema": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "typescript": "catalog:",
    "@types/node": "^25.3.2",
    "rimraf": "^6.1.3",
    "@contractspec/tool.bun": "workspace:*"
  },
  "peerDependencies": {
    "@contractspec/lib.contracts-spec": "^3.0.0",
    "@contractspec/lib.schema": "^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "default": "./dist/index.js"
      },
      "./config": {
        "types": "./dist/config.d.ts",
        "bun": "./dist/config.js",
        "node": "./dist/node/config.js",
        "default": "./dist/config.js"
      },
      "./generator": {
        "types": "./dist/generator.d.ts",
        "bun": "./dist/generator.js",
        "node": "./dist/node/generator.js",
        "default": "./dist/generator.js"
      },
      "./types": {
        "types": "./dist/types.d.ts",
        "bun": "./dist/types.js",
        "node": "./dist/node/types.js",
        "default": "./dist/types.js"
      }
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/integrations/example-generator"
  },
  "homepage": "https://contractspec.io",
  "bugs": {
    "url": "https://github.com/lssm-tech/contractspec/issues"
  },
  "author": {
    "name": "ContractSpec Team",
    "email": "team@contractspec.io"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}

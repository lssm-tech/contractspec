{
  "name": "@contractspec/integration.runtime",
  "version": "2.6.0",
  "description": "Runtime integration with secret management",
  "keywords": [
    "contractspec",
    "integration",
    "runtime",
    "secrets",
    "typescript"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "dev": "contractspec-bun-build dev",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test --pass-with-no-tests",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contractspec/lib.contracts-spec": "workspace:*",
    "@contractspec/lib.contracts-integrations": "workspace:*",
    "@contractspec/lib.logger": "workspace:*",
    "@google-cloud/secret-manager": "^6.1.1",
    "google-gax": "^5.0.0"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "typescript": "catalog:",
    "@contractspec/tool.bun": "workspace:*"
  },
  "exports": {
    ".": "./src/index.ts",
    "./health": "./src/health.ts",
    "./runtime": "./src/runtime.ts",
    "./secrets": "./src/secrets/index.ts",
    "./secrets/env-secret-provider": "./src/secrets/env-secret-provider.ts",
    "./secrets/gcp-secret-manager": "./src/secrets/gcp-secret-manager.ts",
    "./secrets/index": "./src/secrets/index.ts",
    "./secrets/manager": "./src/secrets/manager.ts",
    "./secrets/provider": "./src/secrets/provider.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "default": "./dist/index.js"
      },
      "./health": {
        "types": "./dist/health.d.ts",
        "bun": "./dist/health.js",
        "node": "./dist/node/health.js",
        "default": "./dist/health.js"
      },
      "./runtime": {
        "types": "./dist/runtime.d.ts",
        "bun": "./dist/runtime.js",
        "node": "./dist/node/runtime.js",
        "default": "./dist/runtime.js"
      },
      "./secrets": {
        "types": "./dist/secrets/index.d.ts",
        "bun": "./dist/secrets/index.js",
        "node": "./dist/node/secrets/index.js",
        "default": "./dist/secrets/index.js"
      },
      "./secrets/env-secret-provider": {
        "types": "./dist/secrets/env-secret-provider.d.ts",
        "bun": "./dist/secrets/env-secret-provider.js",
        "node": "./dist/node/secrets/env-secret-provider.js",
        "default": "./dist/secrets/env-secret-provider.js"
      },
      "./secrets/gcp-secret-manager": {
        "types": "./dist/secrets/gcp-secret-manager.d.ts",
        "bun": "./dist/secrets/gcp-secret-manager.js",
        "node": "./dist/node/secrets/gcp-secret-manager.js",
        "default": "./dist/secrets/gcp-secret-manager.js"
      },
      "./secrets/index": {
        "types": "./dist/secrets/index.d.ts",
        "bun": "./dist/secrets/index.js",
        "node": "./dist/node/secrets/index.js",
        "default": "./dist/secrets/index.js"
      },
      "./secrets/manager": {
        "types": "./dist/secrets/manager.d.ts",
        "bun": "./dist/secrets/manager.js",
        "node": "./dist/node/secrets/manager.js",
        "default": "./dist/secrets/manager.js"
      },
      "./secrets/provider": {
        "types": "./dist/secrets/provider.d.ts",
        "bun": "./dist/secrets/provider.js",
        "node": "./dist/node/secrets/provider.js",
        "default": "./dist/secrets/provider.js"
      }
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/integrations/runtime"
  },
  "homepage": "https://contractspec.io"
}

{
  "name": "@contractspec/module.audit-trail",
  "version": "1.45.2",
  "description": "Audit trail module for tracking and querying system events",
  "keywords": [
    "contractspec",
    "audit",
    "logging",
    "compliance",
    "events",
    "typescript"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "dependencies": {
    "@contractspec/lib.schema": "workspace:*",
    "@contractspec/lib.contracts": "workspace:*",
    "@contractspec/lib.bus": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "@contractspec/tool.tsdown": "workspace:*",
    "typescript": "catalog:"
  },
  "exports": {
    ".": "./dist/index.js",
    "./audit-trail.feature": "./dist/audit-trail.feature.js",
    "./contracts": "./dist/contracts/index.js",
    "./entities": "./dist/entities/index.js",
    "./storage": "./dist/storage/index.js",
    "./*": "./*"
  },
  "module": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./audit-trail.feature": "./dist/audit-trail.feature.js",
      "./contracts": "./dist/contracts/index.js",
      "./entities": "./dist/entities/index.js",
      "./storage": "./dist/storage/index.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/modules/audit-trail"
  },
  "homepage": "https://contractspec.io"
}

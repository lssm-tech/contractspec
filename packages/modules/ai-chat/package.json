{
  "name": "@contractspec/module.ai-chat",
  "version": "1.46.2",
  "description": "AI chat interface module",
  "keywords": [
    "contractspec",
    "ai",
    "chat",
    "conversational",
    "typescript"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test"
  },
  "dependencies": {
    "@contractspec/lib.ai-agent": "workspace:*",
    "@contractspec/lib.ai-providers": "workspace:*",
    "@contractspec/lib.contracts": "workspace:*",
    "@contractspec/lib.schema": "workspace:*",
    "@contractspec/lib.metering": "workspace:*",
    "@contractspec/lib.cost-tracking": "workspace:*",
    "@contractspec/lib.design-system": "workspace:*",
    "@contractspec/lib.ui-kit-web": "workspace:*",
    "@ai-sdk/react": "catalog:ai",
    "ai": "catalog:ai",
    "lucide-react": "^0.562.0",
    "react": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@contractspec/tool.tsdown": "workspace:*",
    "@contractspec/tool.typescript": "workspace:*",
    "@types/react": "^19.2.8",
    "tsdown": "catalog:build",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./context": "./dist/context/index.js",
    "./core": "./dist/core/index.js",
    "./presentation": "./dist/presentation/index.js",
    "./presentation/components": "./dist/presentation/components/index.js",
    "./presentation/hooks": "./dist/presentation/hooks/index.js",
    "./providers": "./dist/providers/index.js",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./context": "./dist/context/index.js",
      "./core": "./dist/core/index.js",
      "./presentation": "./dist/presentation/index.js",
      "./presentation/components": "./dist/presentation/components/index.js",
      "./presentation/hooks": "./dist/presentation/hooks/index.js",
      "./providers": "./dist/providers/index.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/modules/ai-chat"
  },
  "homepage": "https://contractspec.io"
}

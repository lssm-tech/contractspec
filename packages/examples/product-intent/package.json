{
  "name": "@contractspec/example.product-intent",
  "version": "1.56.1",
  "description": "Product intent example: evidence ingestion and prompt-ready outputs.",
  "keywords": [
    "contractspec",
    "product-intent",
    "pm",
    "discovery",
    "example"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./example": "./dist/example.js",
    "./load-evidence": "./dist/load-evidence.js",
    "./script": "./dist/script.js",
    "./sync-actions": "./dist/sync-actions.js",
    "./*": "./*"
  },
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test"
  },
  "dependencies": {
    "@contractspec/lib.contracts": "workspace:*",
    "@contractspec/lib.ai-agent": "workspace:*",
    "@contractspec/lib.product-intent-utils": "workspace:*",
    "@contractspec/integration.providers-impls": "workspace:*"
  },
  "devDependencies": {
    "@contractspec/tool.tsdown": "workspace:*",
    "@contractspec/tool.typescript": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./example": "./dist/example.js",
      "./load-evidence": "./dist/load-evidence.js",
      "./script": "./dist/script.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/examples/product-intent"
  },
  "homepage": "https://contractspec.io"
}

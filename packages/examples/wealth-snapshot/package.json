{
  "name": "@lssm/example.wealth-snapshot",
  "version": "1.43.4",
  "description": "Wealth Snapshot mini-app for accounts, assets, liabilities, and goals",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "dependencies": {
    "@lssm/lib.schema": "workspace:*",
    "@lssm/lib.contracts": "workspace:*",
    "@lssm/lib.bus": "workspace:*",
    "@lssm/lib.identity-rbac": "workspace:*",
    "@lssm/module.audit-trail": "workspace:*",
    "@lssm/module.notifications": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@lssm/tool.typescript": "workspace:*",
    "@lssm/tool.tsdown": "workspace:*",
    "typescript": "catalog:"
  },
  "exports": {
    ".": "./dist/index.js",
    "./docs": "./dist/docs/index.js",
    "./docs/wealth-snapshot.docblock": "./dist/docs/wealth-snapshot.docblock.js",
    "./entities": "./dist/entities/index.js",
    "./events": "./dist/events.js",
    "./example": "./dist/example.js",
    "./handlers": "./dist/handlers/index.js",
    "./operations": "./dist/operations/index.js",
    "./presentations": "./dist/presentations/index.js",
    "./wealth-snapshot.feature": "./dist/wealth-snapshot.feature.js",
    "./*": "./*"
  },
  "module": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./contracts": "./dist/contracts/index.js",
      "./docs": "./dist/docs/index.js",
      "./docs/wealth-snapshot.docblock": "./dist/docs/wealth-snapshot.docblock.js",
      "./entities": "./dist/entities/index.js",
      "./events": "./dist/events.js",
      "./example": "./dist/example.js",
      "./handlers": "./dist/handlers/index.js",
      "./presentations": "./dist/presentations/index.js",
      "./wealth-snapshot.feature": "./dist/wealth-snapshot.feature.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/examples/wealth-snapshot"
  },
  "homepage": "https://contractspec.io"
}

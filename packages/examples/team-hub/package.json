{
  "name": "@lssm/example.team-hub",
  "version": "1.41.1",
  "description": "Team Hub example with spaces, tasks, rituals, and announcements",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:canary:pkg": "bun publish:pkg --canary",
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "dependencies": {
    "@lssm/lib.schema": "workspace:*",
    "@lssm/lib.contracts": "workspace:*",
    "@lssm/lib.bus": "workspace:*",
    "@lssm/lib.identity-rbac": "workspace:*",
    "@lssm/lib.jobs": "workspace:*",
    "@lssm/module.audit-trail": "workspace:*",
    "@lssm/module.notifications": "workspace:*",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@lssm/tool.typescript": "workspace:*",
    "@lssm/tool.tsdown": "workspace:*",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": "./src/index.ts",
    "./announcement": "./src/announcement/index.ts",
    "./announcement/announcement.contracts": "./src/announcement/announcement.contracts.ts",
    "./announcement/announcement.schema": "./src/announcement/announcement.schema.ts",
    "./docs": "./src/docs/index.ts",
    "./docs/team-hub.docblock": "./src/docs/team-hub.docblock.ts",
    "./entities": "./src/entities/index.ts",
    "./events": "./src/events.ts",
    "./example": "./src/example.ts",
    "./handlers": "./src/handlers/index.ts",
    "./presentations": "./src/presentations.ts",
    "./ritual": "./src/ritual/index.ts",
    "./ritual/ritual.contracts": "./src/ritual/ritual.contracts.ts",
    "./ritual/ritual.schema": "./src/ritual/ritual.schema.ts",
    "./space": "./src/space/index.ts",
    "./space/space.contracts": "./src/space/space.contracts.ts",
    "./space/space.schema": "./src/space/space.schema.ts",
    "./task": "./src/task/index.ts",
    "./task/task.contracts": "./src/task/task.contracts.ts",
    "./task/task.schema": "./src/task/task.schema.ts",
    "./team-hub.feature": "./src/team-hub.feature.ts",
    "./*": "./*"
  },
  "module": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./announcement": "./dist/announcement/index.js",
      "./announcement/announcement.contracts": "./dist/announcement/announcement.contracts.js",
      "./announcement/announcement.schema": "./dist/announcement/announcement.schema.js",
      "./docs": "./dist/docs/index.js",
      "./docs/team-hub.docblock": "./dist/docs/team-hub.docblock.js",
      "./entities": "./dist/entities/index.js",
      "./events": "./dist/events.js",
      "./example": "./dist/example.js",
      "./handlers": "./dist/handlers/index.js",
      "./presentations": "./dist/presentations/index.js",
      "./ritual": "./dist/ritual/index.js",
      "./ritual/ritual.contracts": "./dist/ritual/ritual.contracts.js",
      "./ritual/ritual.schema": "./dist/ritual/ritual.schema.js",
      "./space": "./dist/space/index.js",
      "./space/space.contracts": "./dist/space/space.contracts.js",
      "./space/space.schema": "./dist/space/space.schema.js",
      "./task": "./dist/task/index.js",
      "./task/task.contracts": "./dist/task/task.contracts.js",
      "./task/task.schema": "./dist/task/task.schema.js",
      "./team-hub.feature": "./dist/team-hub.feature.js",
      "./*": "./*"
    }
  }
}

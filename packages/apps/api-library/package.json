{
  "name": "@contractspec/app.api-library",
  "version": "1.15.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "dev": "bun run --watch src/server.ts",
    "build": "bun run build:types && bun run build:bundle",
    "build:bundle": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile dist/server src/index.ts",
    "build:types": "tsc --noEmit",
    "start": "./server"
  },
  "dependencies": {
    "@contractspec/bundle.library": "workspace:*",
    "@contractspec/bundle.marketing": "workspace:*",
    "@contractspec/bundle.workspace": "workspace:*",
    "@contractspec/lib.contracts": "workspace:*",
    "elysia": "^1.4.21",
    "zod": "^4.3.5"
  },
  "devDependencies": {},
  "module": "src/index.ts",
  "type": "module"
}

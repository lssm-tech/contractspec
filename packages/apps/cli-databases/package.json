{
  "name": "@lssm/app.cli-databases",
  "version": "1.41.1",
  "type": "module",
  "bin": {
    "databases": "dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --canary",
    "build": "tsdown",
    "dev": "bun run build --watch"
  },
  "dependencies": {
    "minimist": "^1.2.8",
    "execa": "^9.6.1"
  },
  "devDependencies": {
    "@lssm/tool.tsdown": "workspace:*",
    "tsdown": "^0.17.4"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli.ts",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.mjs",
      "./cli": "./dist/cli.mjs",
      "./*": "./*"
    }
  }
}

{
  "name": "@contractspec/lib.source-extractors",
  "version": "1.3.0",
  "description": "Extract contract candidates from TypeScript source code across multiple frameworks (NestJS, Express, Fastify, Hono, Elysia, tRPC, Next.js)",
  "keywords": [
    "contractspec",
    "source-extraction",
    "nestjs",
    "express",
    "fastify",
    "hono",
    "elysia",
    "trpc",
    "typescript"
  ],
  "type": "module",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "dev": "contractspec-bun-build dev",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contractspec/lib.contracts-spec": "workspace:*",
    "@contractspec/lib.schema": "workspace:*",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "@contractspec/tool.bun": "workspace:*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./codegen": "./src/codegen/index.ts",
    "./codegen/index": "./src/codegen/index.ts",
    "./extractors": "./src/extractors/index.ts",
    "./extractors/index": "./src/extractors/index.ts",
    "./types": "./src/types.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "browser": "./dist/browser/index.js",
        "default": "./dist/index.js"
      },
      "./codegen": {
        "types": "./dist/codegen/index.d.ts",
        "bun": "./dist/codegen/index.js",
        "node": "./dist/node/codegen/index.js",
        "browser": "./dist/browser/codegen/index.js",
        "default": "./dist/codegen/index.js"
      },
      "./codegen/index": {
        "types": "./dist/codegen/index.d.ts",
        "bun": "./dist/codegen/index.js",
        "node": "./dist/node/codegen/index.js",
        "browser": "./dist/browser/codegen/index.js",
        "default": "./dist/codegen/index.js"
      },
      "./extractors": {
        "types": "./dist/extractors/index.d.ts",
        "bun": "./dist/extractors/index.js",
        "node": "./dist/node/extractors/index.js",
        "browser": "./dist/browser/extractors/index.js",
        "default": "./dist/extractors/index.js"
      },
      "./extractors/index": {
        "types": "./dist/extractors/index.d.ts",
        "bun": "./dist/extractors/index.js",
        "node": "./dist/node/extractors/index.js",
        "browser": "./dist/browser/extractors/index.js",
        "default": "./dist/extractors/index.js"
      },
      "./types": {
        "types": "./dist/types.d.ts",
        "bun": "./dist/types.js",
        "node": "./dist/node/types.js",
        "browser": "./dist/browser/types.js",
        "default": "./dist/types.js"
      }
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/source-extractors"
  },
  "homepage": "https://contractspec.io"
}

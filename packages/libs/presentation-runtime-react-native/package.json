{
  "name": "@lssm/lib.presentation-runtime-react-native",
  "version": "13.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/nativewind-env.d.ts",
  "sideEffects": false,
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit -p tsconfig.json",
    "dev": "bun run build:bundle --watch",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "peerDependencies": {
    "react": "^19.2.1",
    "react-native": "*",
    "react-hook-form": "^7.68.0",
    "zod": "^4.1.13",
    "@lssm/lib.presentation-runtime-core": "workspace:*"
  },
  "dependencies": {
    "@lssm/lib.presentation-runtime-core": "workspace:*",
    "@lssm/lib.ui-kit": "workspace:*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./*": "./*"
    }
  }
}

{
  "name": "@lssm/lib.ai-agent",
  "version": "0.4.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun run"
  },
  "dependencies": {
    "ai": "beta",
    "@ai-sdk/mistral": "beta",
    "@ai-sdk/openai": "beta",
    "@ai-sdk/anthropic": "beta",
    "@lssm/lib.contracts": "workspace:*"
  },
  "devDependencies": {
    "@lssm/tool.tsdown": "workspace:*",
    "@lssm/tool.typescript": "workspace:*",
    "tsdown": "^0.16.8",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": "./dist/index.js",
    "./approval": "./dist/approval/index.js",
    "./approval/workflow": "./dist/approval/workflow.js",
    "./memory": "./dist/memory/index.js",
    "./memory/in-memory": "./dist/memory/in-memory.js",
    "./memory/manager": "./dist/memory/manager.js",
    "./runner": "./dist/runner.js",
    "./spec": "./dist/spec.js",
    "./tools": "./dist/tools/index.js",
    "./tools/executor": "./dist/tools/executor.js",
    "./types": "./dist/types.js",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  }
}

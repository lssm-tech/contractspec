{
  "name": "@lssm/lib.ui-link",
  "version": "1.12.0",
  "type": "module",
  "main": "./dist/link.js",
  "module": "./dist/link.js",
  "types": "./dist/link.d.ts",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "build": "bun run build:bundle && bun run build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun run build:bundle --watch",
    "clean": "rm -rf dist",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint ui --fix",
    "lint:check": "eslint ui"
  },
  "dependencies": {
    "@lssm/lib.ui-kit-core": "workspace:*",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@lssm/tool.typescript": "workspace:*",
    "@lssm/tool.tsdown": "workspace:*",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./ui/link.tsx",
    "./*": "./*"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/link.js",
      "./*": "./*"
    }
  }
}

{
  "name": "@lssm/lib.accessibility",
  "version": "0.0.0",
  "scripts": {
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "dependencies": {
    "@lssm/lib.design-system": "workspace:*",
    "@lssm/lib.ui-kit": "workspace:*",
    "@lssm/lib.ui-kit-web": "workspace:*"
  },
  "devDependencies": {
    "@lssm/tool.tsdown": "workspace:*",
    "@lssm/tool.typescript": "workspace:*",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "next": "15.5.6"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./AccessibilityPanel": "./dist/AccessibilityPanel.js",
    "./AccessibilityProvider": "./dist/AccessibilityProvider.js",
    "./next-route-announcer": "./dist/next-route-announcer.js",
    "./preferences": "./dist/preferences.js",
    "./*": "./*"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./dist/index.js",
    "./src/styles.css"
  ],
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./AccessibilityPanel": "./dist/AccessibilityPanel.js",
      "./AccessibilityProvider": "./dist/AccessibilityProvider.js",
      "./next-route-announcer": "./dist/next-route-announcer.js",
      "./preferences": "./dist/preferences.js",
      "./*": "./*"
    }
  }
}

{
  "name": "@contractspec/lib.accessibility",
  "version": "1.45.3",
  "description": "WCAG compliance utilities and validators",
  "keywords": [
    "contractspec",
    "accessibility",
    "a11y",
    "wcag",
    "react",
    "typescript"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "dependencies": {
    "@contractspec/lib.design-system": "workspace:*",
    "@contractspec/lib.ui-kit": "workspace:*",
    "@contractspec/lib.ui-kit-web": "workspace:*"
  },
  "devDependencies": {
    "@contractspec/tool.tsdown": "workspace:*",
    "@contractspec/tool.typescript": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "19.2.3",
    "next": "16.1.1"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./dist/index.js",
    "./dist/*.css"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./AccessibilityPanel": "./dist/AccessibilityPanel.js",
    "./AccessibilityProvider": "./dist/AccessibilityProvider.js",
    "./next-route-announcer": "./dist/next-route-announcer.js",
    "./preferences": "./dist/preferences.js",
    "./*": "./*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./AccessibilityPanel": "./dist/AccessibilityPanel.js",
      "./AccessibilityProvider": "./dist/AccessibilityProvider.js",
      "./next-route-announcer": "./dist/next-route-announcer.js",
      "./preferences": "./dist/preferences.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/accessibility"
  },
  "homepage": "https://contractspec.io"
}

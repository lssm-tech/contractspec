{
  "name": "@contractspec/lib.example-shared-ui",
  "version": "1.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@apollo/client": "^4.0.12",
    "@contractspec/lib.contracts": "workspace:*",
    "@contractspec/lib.design-system": "workspace:*",
    "@contractspec/lib.ui-kit-web": "workspace:*",
    "@tanstack/react-query": "^5.90.16",
    "framer-motion": "^12.26.1",
    "lucide-react": "^0.300.0",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.39.2",
    "typescript": "catalog:"
  }
}

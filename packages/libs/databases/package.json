{
  "name": "@lssm/lib.databases",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "databases": "dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsdown",
    "dev": "bun run build --watch"
  },
  "dependencies": {
    "minimist": "^1.2.8",
    "execa": "^9.5.1"
  },
  "devDependencies": {
    "@lssm/tool.tsdown": "workspace:*",
    "tsdown": "^0.15.9"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli.js",
    "./*": "./*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "./*.css": "./dist/*.css",
      "./package.json": "./package.json"
    }
  }
}

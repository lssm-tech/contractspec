{
  "name": "@contractspec/lib.overlay-engine",
  "version": "1.61.0",
  "description": "Runtime overlay engine for ContractSpec personalization and adaptive UI rendering.",
  "keywords": [
    "contractspec",
    "overlay",
    "personalization",
    "ui",
    "rendering",
    "typescript"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "dev": "contractspec-bun-build dev",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test --pass-with-no-tests",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contractspec/lib.contracts": "workspace:*",
    "fast-json-stable-stringify": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "typescript": "catalog:",
    "@contractspec/tool.bun": "workspace:*"
  },
  "exports": {
    ".": "./src/index.ts",
    "./merger": "./src/merger.ts",
    "./react": "./src/react.ts",
    "./registry": "./src/registry.ts",
    "./runtime": "./src/runtime.ts",
    "./signer": "./src/signer.ts",
    "./spec": "./src/spec.ts",
    "./types": "./src/types.ts",
    "./validator": "./src/validator.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "default": "./dist/index.js"
      },
      "./merger": {
        "types": "./dist/merger.d.ts",
        "bun": "./dist/merger.js",
        "node": "./dist/node/merger.js",
        "default": "./dist/merger.js"
      },
      "./react": {
        "types": "./dist/react.d.ts",
        "bun": "./dist/react.js",
        "node": "./dist/node/react.js",
        "default": "./dist/react.js"
      },
      "./registry": {
        "types": "./dist/registry.d.ts",
        "bun": "./dist/registry.js",
        "node": "./dist/node/registry.js",
        "default": "./dist/registry.js"
      },
      "./runtime": {
        "types": "./dist/runtime.d.ts",
        "bun": "./dist/runtime.js",
        "node": "./dist/node/runtime.js",
        "default": "./dist/runtime.js"
      },
      "./signer": {
        "types": "./dist/signer.d.ts",
        "bun": "./dist/signer.js",
        "node": "./dist/node/signer.js",
        "default": "./dist/signer.js"
      },
      "./spec": {
        "types": "./dist/spec.d.ts",
        "bun": "./dist/spec.js",
        "node": "./dist/node/spec.js",
        "default": "./dist/spec.js"
      },
      "./types": {
        "types": "./dist/types.d.ts",
        "bun": "./dist/types.js",
        "node": "./dist/node/types.js",
        "default": "./dist/types.js"
      },
      "./validator": {
        "types": "./dist/validator.d.ts",
        "bun": "./dist/validator.js",
        "node": "./dist/node/validator.js",
        "default": "./dist/validator.js"
      }
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/overlay-engine"
  },
  "homepage": "https://contractspec.io"
}

{
  "name": "@contractspec/lib.ai-providers",
  "version": "1.60.0",
  "description": "Unified AI provider abstraction layer",
  "keywords": [
    "contractspec",
    "ai",
    "llm",
    "providers",
    "openai",
    "anthropic",
    "typescript"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "dev": "contractspec-bun-build dev",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "catalog:ai",
    "@ai-sdk/google": "catalog:ai",
    "@ai-sdk/mistral": "catalog:ai",
    "@ai-sdk/openai": "catalog:ai",
    "ai": "catalog:ai",
    "ollama-ai-provider": "catalog:ai",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "typescript": "catalog:",
    "@contractspec/tool.bun": "workspace:*"
  },
  "exports": {
    ".": "./src/index.ts",
    "./factory": "./src/factory.ts",
    "./legacy": "./src/legacy.ts",
    "./models": "./src/models.ts",
    "./types": "./src/types.ts",
    "./validation": "./src/validation.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "browser": "./dist/browser/index.js",
        "default": "./dist/index.js"
      },
      "./factory": {
        "types": "./dist/factory.d.ts",
        "bun": "./dist/factory.js",
        "node": "./dist/node/factory.js",
        "browser": "./dist/browser/factory.js",
        "default": "./dist/factory.js"
      },
      "./legacy": {
        "types": "./dist/legacy.d.ts",
        "bun": "./dist/legacy.js",
        "node": "./dist/node/legacy.js",
        "browser": "./dist/browser/legacy.js",
        "default": "./dist/legacy.js"
      },
      "./models": {
        "types": "./dist/models.d.ts",
        "bun": "./dist/models.js",
        "node": "./dist/node/models.js",
        "browser": "./dist/browser/models.js",
        "default": "./dist/models.js"
      },
      "./types": {
        "types": "./dist/types.d.ts",
        "bun": "./dist/types.js",
        "node": "./dist/node/types.js",
        "browser": "./dist/browser/types.js",
        "default": "./dist/types.js"
      },
      "./validation": {
        "types": "./dist/validation.d.ts",
        "bun": "./dist/validation.js",
        "node": "./dist/node/validation.js",
        "browser": "./dist/browser/validation.js",
        "default": "./dist/validation.js"
      }
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/ai-providers"
  },
  "homepage": "https://contractspec.io"
}

{
  "name": "@lssm/lib.ai-providers",
  "version": "1.42.8",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "catalog:ai",
    "@ai-sdk/google": "catalog:ai",
    "@ai-sdk/mistral": "catalog:ai",
    "@ai-sdk/openai": "catalog:ai",
    "ai": "catalog:ai",
    "ollama-ai-provider": "catalog:ai",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@lssm/tool.tsdown": "workspace:*",
    "@lssm/tool.typescript": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:"
  },
  "exports": {
    ".": "./dist/index.js",
    "./factory": "./dist/factory.js",
    "./legacy": "./dist/legacy.js",
    "./models": "./dist/models.js",
    "./types": "./dist/types.js",
    "./validation": "./dist/validation.js",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./factory": "./dist/factory.js",
      "./legacy": "./dist/legacy.js",
      "./models": "./dist/models.js",
      "./types": "./dist/types.js",
      "./validation": "./dist/validation.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/ai-providers"
  }
}

  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js",
        "require": "./dist/*.cjs",
        "default": "./dist/*.js"
      },
      "./*.css": "./dist/*.css",
      "./package.json": "./package.json"
    },
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ]

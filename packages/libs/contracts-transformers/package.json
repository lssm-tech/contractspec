{
  "name": "@contractspec/lib.contracts-transformers",
  "version": "2.9.0",
  "description": "Contract format transformations: import/export between ContractSpec and external formats (OpenAPI, AsyncAPI, etc.)",
  "keywords": [
    "contractspec",
    "openapi",
    "asyncapi",
    "transformers",
    "code-generation",
    "typescript"
  ],
  "type": "module",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "dev": "contractspec-bun-build dev",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun test",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@contractspec/lib.contracts-spec": "workspace:*",
    "@contractspec/lib.schema": "workspace:*",
    "compare-versions": "^6.1.1",
    "openapi-types": "^12.1.3",
    "yaml": "^2.7.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "typescript": "catalog:",
    "@contractspec/tool.bun": "workspace:*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./common": "./src/common/index.ts",
    "./common/index": "./src/common/index.ts",
    "./openapi": "./src/openapi/index.ts",
    "./openapi/index": "./src/openapi/index.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "browser": "./dist/browser/index.js",
        "default": "./dist/index.js"
      },
      "./common": {
        "types": "./dist/common/index.d.ts",
        "bun": "./dist/common/index.js",
        "node": "./dist/node/common/index.js",
        "browser": "./dist/browser/common/index.js",
        "default": "./dist/common/index.js"
      },
      "./common/index": {
        "types": "./dist/common/index.d.ts",
        "bun": "./dist/common/index.js",
        "node": "./dist/node/common/index.js",
        "browser": "./dist/browser/common/index.js",
        "default": "./dist/common/index.js"
      },
      "./openapi": {
        "types": "./dist/openapi/index.d.ts",
        "bun": "./dist/openapi/index.js",
        "node": "./dist/node/openapi/index.js",
        "browser": "./dist/browser/openapi/index.js",
        "default": "./dist/openapi/index.js"
      },
      "./openapi/index": {
        "types": "./dist/openapi/index.d.ts",
        "bun": "./dist/openapi/index.js",
        "node": "./dist/node/openapi/index.js",
        "browser": "./dist/browser/openapi/index.js",
        "default": "./dist/openapi/index.js"
      }
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/contracts-transformers"
  },
  "homepage": "https://contractspec.io"
}

{
  "name": "@lssm/lib.support-bot",
  "version": "1.43.4",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun run"
  },
  "dependencies": {
    "@lssm/lib.ai-agent": "workspace:*",
    "@lssm/lib.contracts": "workspace:*",
    "@lssm/lib.knowledge": "workspace:*",
    "@lssm/lib.schema": "workspace:*"
  },
  "devDependencies": {
    "@lssm/tool.tsdown": "workspace:*",
    "@lssm/tool.typescript": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:"
  },
  "exports": {
    ".": "./dist/index.js",
    "./bot": "./dist/bot/index.js",
    "./bot/auto-responder": "./dist/bot/auto-responder.js",
    "./bot/feedback-loop": "./dist/bot/feedback-loop.js",
    "./bot/tools": "./dist/bot/tools.js",
    "./rag": "./dist/rag/index.js",
    "./rag/ticket-resolver": "./dist/rag/ticket-resolver.js",
    "./spec": "./dist/spec.js",
    "./tickets": "./dist/tickets/index.js",
    "./tickets/classifier": "./dist/tickets/classifier.js",
    "./types": "./dist/types.js",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./bot": "./dist/bot/index.js",
      "./bot/auto-responder": "./dist/bot/auto-responder.js",
      "./bot/feedback-loop": "./dist/bot/feedback-loop.js",
      "./bot/tools": "./dist/bot/tools.js",
      "./rag": "./dist/rag/index.js",
      "./rag/ticket-resolver": "./dist/rag/ticket-resolver.js",
      "./spec": "./dist/spec.js",
      "./tickets": "./dist/tickets/index.js",
      "./tickets/classifier": "./dist/tickets/classifier.js",
      "./types": "./dist/types.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/support-bot"
  },
  "homepage": "https://contractspec.io"
}

{
  "name": "@contractspec/lib.content-gen",
  "version": "1.45.5",
  "description": "AI-powered content generation for blog, email, and social",
  "keywords": [
    "contractspec",
    "content",
    "ai",
    "generation",
    "seo",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "test": "bun run"
  },
  "dependencies": {
    "@contractspec/lib.contracts": "workspace:*"
  },
  "devDependencies": {
    "@contractspec/tool.tsdown": "workspace:*",
    "@contractspec/tool.typescript": "workspace:*",
    "tsdown": "catalog:build",
    "typescript": "catalog:"
  },
  "exports": {
    ".": "./dist/index.js",
    "./generators": "./dist/generators/index.js",
    "./generators/blog": "./dist/generators/blog.js",
    "./generators/email": "./dist/generators/email.js",
    "./generators/landing-page": "./dist/generators/landing-page.js",
    "./generators/social": "./dist/generators/social.js",
    "./seo": "./dist/seo/index.js",
    "./seo/optimizer": "./dist/seo/optimizer.js",
    "./types": "./dist/types.js",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./generators": "./dist/generators/index.js",
      "./generators/blog": "./dist/generators/blog.js",
      "./generators/email": "./dist/generators/email.js",
      "./generators/landing-page": "./dist/generators/landing-page.js",
      "./generators/social": "./dist/generators/social.js",
      "./seo": "./dist/seo/index.js",
      "./seo/optimizer": "./dist/seo/optimizer.js",
      "./types": "./dist/types.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/content-gen"
  },
  "homepage": "https://contractspec.io"
}

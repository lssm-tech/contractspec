    "./generated/prisma/internal/prismaNamespaceBrowser": "./src/generated/prisma/internal/prismaNamespaceBrowser.ts",
    "./generated/prisma/models": "./src/generated/prisma/models.ts",
    "./generated/prisma/models/Account": "./src/generated/prisma/models/Account.ts",
    "./generated/prisma/models/ApiKey": "./src/generated/prisma/models/ApiKey.ts",
    "./generated/prisma/models/Invitation": "./src/generated/prisma/models/Invitation.ts",
    "./generated/prisma/models/Member": "./src/generated/prisma/models/Member.ts",

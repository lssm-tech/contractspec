{
  "name": "@lssm/lib.database-contractspec-studio",
  "version": "0.4.0",
  "scripts": {
    "clean": "rm -rf dist src/generated",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "build": "bun run build:bundle",
    "build:types": "tsc --noEmit",
    "build:bundle": "tsdown",
    "dev": "bun run build:bundle --watch",
    "prisma:import": "bun database import --target .",
    "prisma:check": "bun database check --target .",
    "db:migrate": "bun database migrate:dev",
    "db:deploy": "bun database migrate:deploy",
    "db:generate": "bun prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "bun database seed",
    "db:reset": "prisma db push --skip-generate --force-reset && bun run db:seed",
    "db:reset:full": "bun run db:generate && bun run db:reset",
    "generate": "bun run db:generate"
  },
  "peerDependencies": {
    "@prisma/client": "^7.0.1"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1"
  },
  "devDependencies": {
    "@lssm/app.cli-database": "workspace:*",
    "prisma": "^7.0.1",
    "tsdown": "^0.16.8"
  },
  "main": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./client": "./dist/client.mjs",
    "./enums": "./dist/enums.mjs",
    "./models": "./dist/models.mjs",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "generated"
  ],
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.mjs",
      "./client": "./dist/client.mjs",
      "./enums": "./dist/enums.mjs",
      "./generated/pothos-types": "./dist/generated/pothos-types.mjs",
      "./generated/prisma/browser": "./dist/generated/prisma/browser.mjs",

    "./generated/prisma/models/Passkey": "./src/generated/prisma/models/Passkey.ts",
    "./generated/prisma/models/Permission": "./src/generated/prisma/models/Permission.ts",
    "./generated/prisma/models/PolicyBinding": "./src/generated/prisma/models/PolicyBinding.ts",
    "./generated/prisma/models/Referral": "./src/generated/prisma/models/Referral.ts",
    "./generated/prisma/models/ReferralReward": "./src/generated/prisma/models/ReferralReward.ts",
    "./generated/prisma/models/Role": "./src/generated/prisma/models/Role.ts",
    "./generated/prisma/models/Session": "./src/generated/prisma/models/Session.ts",
    "./generated/prisma/models/SsoProvider": "./src/generated/prisma/models/SsoProvider.ts",
    "./generated/prisma/models/Team": "./src/generated/prisma/models/Team.ts",

{
  "name": "@contractspec/lib.email",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "clean": "rm -rf dist",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit",
    "dev": "contractspec-bun-build dev"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "browser": "./dist/browser/index.js",
        "default": "./dist/index.js"
      },
      "./client": {
        "types": "./dist/client.d.ts",
        "bun": "./dist/client.js",
        "node": "./dist/node/client.js",
        "browser": "./dist/browser/client.js",
        "default": "./dist/client.js"
      },
      "./types": {
        "types": "./dist/types.d.ts",
        "bun": "./dist/types.js",
        "node": "./dist/node/types.js",
        "browser": "./dist/browser/types.js",
        "default": "./dist/types.js"
      },
      "./utils": {
        "types": "./dist/utils.d.ts",
        "bun": "./dist/utils.js",
        "node": "./dist/node/utils.js",
        "browser": "./dist/browser/utils.js",
        "default": "./dist/utils.js"
      }
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./types": "./src/types.ts",
    "./utils": "./src/utils.ts"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@scaleway/sdk": "^3.1.5",
    "@scaleway/sdk-client": "^2.1.0",
    "@contractspec/lib.logger": "workspace:*"
  },
  "devDependencies": {
    "@contractspec/tool.typescript": "workspace:*",
    "typescript": "catalog:",
    "@contractspec/tool.bun": "workspace:*"
  }
}

{
  "name": "@lssm/lib.runtime-local",
  "version": "0.4.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "build": "bun run build:bundle && bun run build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",
    "dev": "bun run build:bundle --watch",
    "clean": "rimraf dist .turbo",
    "lint": "bun lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "dependencies": {
    "@apollo/client": "^4.0.9",
    "@graphql-tools/schema": "^10.0.30",
    "browserify-fs": "^1.0.0",
    "eventemitter3": "^5.0.1",
    "graphql": "^16.9.0",
    "sql.js": "^1.9.0"
  },
  "devDependencies": {
    "@lssm/tool.tsdown": "workspace:*",
    "@lssm/tool.typescript": "workspace:*",
    "@types/sql.js": "^1.4.9",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": "./src/index.ts",
    "./database/sqlite-wasm": "./src/database/sqlite-wasm.ts",
    "./database/sqlite-wasm-stub": "./src/database/sqlite-wasm-stub.ts",
    "./events/local-pubsub": "./src/events/local-pubsub.ts",
    "./graphql/local-client": "./src/graphql/local-client.ts",
    "./handlers": "./src/handlers/index.ts",
    "./handlers/agent.handlers": "./src/handlers/agent.handlers.ts",
    "./handlers/analytics.handlers": "./src/handlers/analytics.handlers.ts",
    "./handlers/crm.handlers": "./src/handlers/crm.handlers.ts",
    "./handlers/integration.handlers": "./src/handlers/integration.handlers.ts",
    "./handlers/marketplace.handlers": "./src/handlers/marketplace.handlers.ts",
    "./handlers/saas.handlers": "./src/handlers/saas.handlers.ts",
    "./handlers/workflow.handlers": "./src/handlers/workflow.handlers.ts",
    "./runtime/services": "./src/runtime/services.ts",
    "./storage/indexeddb": "./src/storage/indexeddb.ts",
    "./utils/id": "./src/utils/id.ts",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./database/sqlite-wasm": "./dist/database/sqlite-wasm.js",
      "./database/sqlite-wasm-stub": "./dist/database/sqlite-wasm-stub.js",
      "./events/local-pubsub": "./dist/events/local-pubsub.js",
      "./graphql/local-client": "./dist/graphql/local-client.js",
      "./handlers": "./dist/handlers/index.js",
      "./handlers/agent.handlers": "./dist/handlers/agent.handlers.js",
      "./handlers/analytics.handlers": "./dist/handlers/analytics.handlers.js",
      "./handlers/crm.handlers": "./dist/handlers/crm.handlers.js",
      "./handlers/integration.handlers": "./dist/handlers/integration.handlers.js",
      "./handlers/marketplace.handlers": "./dist/handlers/marketplace.handlers.js",
      "./handlers/saas.handlers": "./dist/handlers/saas.handlers.js",
      "./handlers/workflow.handlers": "./dist/handlers/workflow.handlers.js",
      "./runtime/services": "./dist/runtime/services.js",
      "./storage/indexeddb": "./dist/storage/indexeddb.js",
      "./utils/id": "./dist/utils/id.js",
      "./*": "./*"
    }
  }
}

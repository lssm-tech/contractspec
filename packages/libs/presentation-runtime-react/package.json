{
  "name": "@contractspec/lib.presentation-runtime-react",
  "version": "1.47.0",
  "description": "React presentation runtime with workflow components",
  "keywords": [
    "contractspec",
    "presentation",
    "react",
    "workflow",
    "ui",
    "typescript"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun build:types && bun build:bundle",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit -p tsconfig.json",
    "dev": "bun run build:bundle --watch",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-hook-form": "^7.70.0",
    "zod": "^4.3.5",
    "@contractspec/lib.presentation-runtime-core": "workspace:*"
  },
  "dependencies": {
    "@contractspec/lib.presentation-runtime-core": "workspace:*",
    "@contractspec/lib.contracts": "workspace:*",
    "@contractspec/lib.design-system": "workspace:*",
    "@contractspec/lib.ui-kit-web": "workspace:*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./useWorkflow": "./dist/useWorkflow.js",
    "./WorkflowStepper": "./dist/WorkflowStepper.js",
    "./WorkflowStepRenderer": "./dist/WorkflowStepRenderer.js",
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./useWorkflow": "./dist/useWorkflow.js",
      "./WorkflowStepper": "./dist/WorkflowStepper.js",
      "./WorkflowStepRenderer": "./dist/WorkflowStepRenderer.js",
      "./*": "./*"
    },
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lssm-tech/contractspec.git",
    "directory": "packages/libs/presentation-runtime-react"
  },
  "homepage": "https://contractspec.io",
  "devDependencies": {
    "react-native-css": "^3.0.0"
  }
}

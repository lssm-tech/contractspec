    "./provisioning": "./dist/provisioning/index.mjs",
    "./provisioning/service": "./dist/provisioning/service.mjs",
    "./rls": "./dist/rls/index.mjs",
    "./rls/prisma-middleware": "./dist/rls/prisma-middleware.mjs",
    "./*": "./*"
  },
  "module": "./dist/index.mjs"

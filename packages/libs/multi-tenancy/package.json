{
  "name": "@lssm/lib.multi-tenancy",
  "version": "0.1.0",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "tsdown",
    "build:types": "tsc --noEmit",

    "dev:android": "bun expo start -c --android",
    "android": "bun expo start -c --android",
    "ios": "bun expo start -c --ios",
    "web": "bun expo start -c --web",
    "relay": "relay-compiler --src . --schema ../api-contractspec/schema.graphql --language typescript --artifactDirectory ./src/__generated__"
